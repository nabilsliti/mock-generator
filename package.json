{
  "name": "mock-generator",
  "version": "0.0.1",
  "description": "Generate a mock data using a schema definition",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "browser": "dist/browser/mock-generator.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && rollup --config rollup.config.ts --bundleConfigAsCjs",
    "doc": " rimraf docs && typedoc",
    "test": "jest",
    "lint": "eslint ./src --ext .ts"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "lorem-ipsum": "^2.0.8",
    "randexp": "^0.5.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "rollup": "4.1.5",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nabilsliti/mock-generator.git"
  },
  "keywords": [
    "mock",
    "fake",
    "api",
    "rest"
  ],
  "author": {
    "name": "Nabil Sliti",
    "email": "nabil.sliti.prod@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabilsliti/mock-generator/issues"
  },
  "homepage": "https://github.com/nabilsliti/mock-generator#readme"
}
